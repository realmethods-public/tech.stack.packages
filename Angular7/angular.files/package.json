#set( $nexusEmail = ${aib.getParam('nexus.email')} )
#set( $nexusRepoUrl = ${aib.getParam('nexus.repoUrl')} )
   	"predeploy": "ng build & npm install -g npm-cli-login",
    "deploy": "npm-cli-login -u ${nexusUserName} -p ${nexusPassword} -e ${nexusEmail} -r ${nexusRepoUrl}",
    "postdeploy" : "npm publish --registry ${nexusRepoUrl}/",
#end##if ( ${aib.getParam('jfrog.inUse')} == "true" )    

