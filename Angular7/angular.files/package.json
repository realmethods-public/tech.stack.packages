#set( $jfrogUserName = ${aib.getParam('jfrog.userName')} )
#set( $jfrogPassword = ${aib.getParam('jfrog.password')} )
#set( $jfrogEmail = ${aib.getParam('jfrog.email')} )
#set( $jfrogRepoUrl = "${jfrogServerUrl}/api/npm/${jfrogNPMRepository}/" )
   	"predeploy": "ng build",
    "deploy": "npm-cli-login -u ${jfrogUserName} -p ${jfrogPassword} -e ${jfrogEmail} -r ${jfrogRepoUrl}",
    "postdeploy" : "npm publish --registry ${jfrogRepoUrl}",
